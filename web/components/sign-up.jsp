<section class="login">
    <div class="container">
        <div class="card sign-up">
            <div class="card-action blue darken-1 white-text">
                <h3>Cadastre-se</h3>
            </div>

            <form class="card-content">
                <div>
                    <fieldset class="form-field">
                        <label for="username">Usuario</label>
                        <input type="text" id="username">
                    </fieldset>

                    <fieldset class="form-field">
                        <label for="password">Senha</label>
                        <input type="password" id="password">
                    </fieldset>

                    <fieldset class="form-field">
                        <label for="username">Nome</label>
                        <input type="text" id="username">
                    </fieldset>

                    <fieldset class="form-field gender">
                        <legend>Genero</legend>

                        <label>
                            <input name="gender" value="masculino" type="radio" checked />
                            <span>Masculino</span>
                        </label>

                        <label>
                            <input name="gender" value="feminino" type="radio" />
                            <span>Feminino</span>
                        </label>

                        <label>
                            <input name="gender" value="outros" type="radio" />
                            <span>Outros</span>
                        </label>
                    </fieldset>
                </div>

                <button type="submit" class="btn btn-large waves-dark blue darken-1">Cadastrar</button>
            </form>
        </div>
    </div>
</section>